function filterFemale(nudiesHTML) {
  const nudie_cards = document.getElementById('gender_filter');
  if (nudie_cards.style.display === "none") {
    nudie_cards.style.display = "block";
  } else {
    nudie_cards.style.display = "none";
  }
  nudie_cards.innerHTML = nudiesHTML;
}

filterFemale('<%= j render "nudies/index_cards", nudies: @nudies %>');

